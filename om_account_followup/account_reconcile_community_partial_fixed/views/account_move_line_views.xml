<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_line_tree_reconcile" model="ir.ui.view">
        <field name="name">account.move.line.reconcile.view</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="after">
                <header>
                    <button name="%(action_partial_reconcile_wizard)d" type="action" string="Partial Reconcile" class="btn-primary"/>
                </header>
            </xpath>
        </field>
    </record>
</odoo>
