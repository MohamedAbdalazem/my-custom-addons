<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">


        <record id="view_inherit_pos_config_form" model="ir.ui.view">
            <field name="name">view.inherit.pos.config.form</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
            <field name="arch" type="xml">

                <xpath expr="//sheet/div[4]/div" position="before">
                    <setting id="internal_transfer" string="Internal Transfer" help="Allow Internal Transfer">
                        <field name="create_internal_transfer"/>
                        <group invisible="not create_internal_transfer">
                            <field name="internal_transfer_auto_confirm"/>
                            <field name="transfer_picking_type_ids" widget="many2many_tags"/>
                            <field name="transfer_location_dest_ids" widget="many2many_tags"/>
                        </group>
                    </setting>
                </xpath>
            </field>
        </record>

    </data>
</odoo>